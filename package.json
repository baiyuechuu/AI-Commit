{
	"name": "aicommit-js",
	"version": "2.0.0",
	"description": "AI-powered git commit message generator with multiple providers and advanced features",
	"type": "module",
	"main": "aicommit.js",
	"bin": {
		"aicommit": "./aicommit.js"
	},
	"scripts": {
		"start": "node aicommit.js",
		"build": "ncc build aicommit.js -o dist --minify",
		"build:executable": "node build.js",
		"install:bin": "rm -f ~/.local/bin/aicommit && cp dist/aicommit ~/.local/bin/aicommit && chmod +x ~/.local/bin/aicommit",
		"lint": "eslint aicommit.js",
		"format": "prettier --write aicommit.js"
	},
	"keywords": [
		"git",
		"commit",
		"ai",
		"automation",
		"cli",
		"openrouter",
		"openai",
		"deepseek",
		"conventional-commits"
	],
	"author": "baiyuechuu",
	"license": "MIT",
	"dependencies": {
		"chalk": "^5.3.0",
		"commander": "^11.1.0",
		"inquirer": "^9.2.12",
		"ora": "^7.0.1"
	},
	"devDependencies": {
		"@vercel/ncc": "^0.38.3",
		"eslint": "^8.55.0",
		"nodemon": "^3.0.2",
		"prettier": "^3.1.0"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/baiyuechuu/AI-Commit.git"
	},
	"bugs": {
		"url": "https://github.com/baiyuechuu/AI-Commit/issues"
	},
	"homepage": "https://github.com/baiyuechuu/AI-Commit#readme"
}
